CREATE PROCEDURE fill_big_table()
BEGIN
DECLARE v1 INT DEFAULT 100;
WHILE v1 > 0 DO
INSERT big_table VALUES (NULL);
SET v1 = v1 - 1;
END WHILE;
END|
CALL fill_big_table();
